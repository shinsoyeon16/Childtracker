<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yonginuniv.childtracker">
    <uses-permission android:name="android.permission.INTERNET" android:required="true"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:windowSoftInputMode="stateAlwaysHidden|adjustNothing"
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data android:name="com.kakao.sdk.AppKey" android:value="f349bfce1ac3ff007ecff89ef34bcb47"/>
        <activity android:name=".InitActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".JoinActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".RegisterDevice"></activity>
        <activity android:name=".ModifyDevice"></activity>
        <activity android:name=".DeveloperInformationActivity"></activity>
    </application>

</manifest>